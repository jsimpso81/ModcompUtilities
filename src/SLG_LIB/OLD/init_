      SUBROUTINE INIT
      IMPLICIT INTEGER(A-Z)
      COMMON /XY/ X,Y
      LOGICAL*1 FNAME(20)
      TYPE 100
  100 FORMAT (1X,'OUTPUT FILE NAME -- ',$)
      ACCEPT 200,LNAME,FNAME
  200 FORMAT (Q,20A1)
      CALL ASSIGN(6,FNAME,LNAME,'NEW','CC',2)
      X=0
      Y=0
      REWIND 2
      RETURN
      END
