C-----SAMP6.FOR
C     DISPLAY CHARACTERS AT VARIOUS ANGLES
      PROGRAM SAMP6
      DIMENSION ICHAR(30),JCHAR(8)
      COMMON /IOUNIT/LU1,LU2,LU3,LU4,LU5
      DATA ICHAR/'C','H','A','R','A','C','T','E','R',' ',
     &           'O','R','I','E','N','T','A','T','I','O','N',' ',
     &           'R','O','T','A','T','I','O','N'/
      DATA JCHAR/'A','N','Y',' ','S','I','Z','E'/
      DATA CH/.0828/
      DATA CW/.0552/
      DATA CH2/.1656/
      DATA CW2/.1104/
      DATA CHZ/.1/
      DATA CWZ/.067/
      DATA SAME/9999.0/
      WRITE (LU2,1101)
 1101 FORMAT (' CHARACTERS AT VARIOUS ANGLES')
      CALL MODE(2,8.,-.25,0.)
      CALL MODE(3,7.25,SAME,SAME)
      CALL MODE(4,CHZ,CWZ,SAME)
      DO 12 I=1,271,90
      ANGLE=I-1
      CALL MODE(4,SAME,SAME,ANGLE)
      CALL NOTE(SAME,SAME,
     &'  ORIENTATION ANGLE 0 - ANNOTATION ANGLE ',41)
   12 CALL NOTE(SAME,SAME,ANGLE,1000)
      CALL MODE(4,CH,CW,0.)
      CALL NOTE(-.2,5.,'ORIENTATION ANGLE 0 - ANNOTATION ANGLE 0',40)
      CALL MODE(5,SAME,90.,SAME)
      CALL NOTE(-.1,5.5,'ORIENTATION ANGLE 90 - ANNOTATION ANGLE 0',41)
      CALL MODE(4,SAME,SAME,180.)
      CALL NOTE(5.3,6.,'ORIENTATION ANGLE 90 - ANNOTATION ANGLE 180',43)
      CALL MODE(5,SAME,0.,SAME)
      CALL NOTE(3.5,6.5,'ORIENTATION ANGLE 0 - ANNOTATION ANGLE 180',42)
      CALL MODE(4,CHZ,CWZ,SAME)
      DO 20 IANGLE=1,331,30
      IANGL1=IANGLE-1
      CALL MODE(4,SAME,SAME,FLOAT(IANGL1))
      CALL NOTE(2.2,2.3,'   ANNOTATION ',14)
   20 CALL NOTE(SAME,SAME,IANGL1,0)
      CALL MODE(4,CH2,CW2,90.)
      CALL MODE(5,-20.,0.,SAME)
      CALL NOTE(5.0,0.5,'-20 DEGREE SLANT',16)
      CALL MODE(5,22.,SAME,SAME)
      CALL NOTE(6.5,SAME,'     +22 DEGREE SLANT',21)
      CALL NOTE(5.5,0.0,' ',1)
      CALL MODE(4,CHZ,CWZ,90.)
      CALL MODE(6,1.,.18,SAME)
      DO 30 I=1,30
      ANGLE=(I-1)*30
      CALL MODE(5,0.,ANGLE,SAME)
   30 CALL NOTE(SAME,SAME,ICHAR(I),1)
      CALL MODE(5,SAME,0.,SAME)
      CALL NOTE(6.5,0.0,' ',1)
      DO 40 I=1,8
      CX=FLOAT(I)*CH
      CY=FLOAT(I)*CW
      CALL MODE(4,CX,CY,SAME)
   40 CALL NOTE(SAME,SAME,JCHAR(I),1)
      CALL MODE(4,CH2,CW,SAME)
      CALL NOTE(7.0,0.0,'HEIGHT  ',8)
      CALL MODE(4,CH,CW2,SAME)
      CALL NOTE(SAME,SAME,'WIDTH  ',7)
      CALL MODE(4,CH,CW,SAME)
      CALL MODE(6,SAME,.2,SAME)
      CALL NOTE(SAME,SAME,'SPACING',7)
      CALL MODE(4,CH2,CW2,SAME)
      CALL NOTE(7.5,0.0,'CHARACTER ',10)
      CALL MODE(6,2.,SAME,SAME)
      CALL NOTE(SAME,SAME,'LINE ',5)
      CALL MODE(6,3.,SAME,SAME)
      CALL NOTE(SAME,SAME,'DOTS',4)
      CALL MODE(4,CH,CW,SAME)
      CALL MODE(6,1.,SAME,SAME)
      ICHR=0
      CX=7.85
      DO 60 J=1,2
      CY=0.0
      DO 50 I=1,64
      CALL NOTE(CX,CY,ICHR,-1)
      ICHR=ICHR+1
   50 CY=CY+.1
   60 CX=CX+.1
      CALL NOTE(7.0,4.0,'REAL = ',7)
      CALL NOTE(SAME,SAME,3.141563,1004)
      CALL NOTE(7.5,4.0,'INTEGER = ',10)
      CALL NOTE(SAME,SAME,-32767,0)
      CALL DRAW(8.,0.,1,9000)
      CALL DRAW(0.,0.,0,9999)
      CALL EXIT
      END
